#!/usr/bin/env node




var cp = require('child_process');

var css = cp.spawn(__dirname+'/sass', ['-w', './style.sass', './assets/style.css'], {
  stdio: ['ignore', process.stdout, process.stderr]
});

var js = cp.spawn(__dirname+'/webpack', ['-w'], {
  stdio: ['ignore', process.stdout, process.stderr]
});

